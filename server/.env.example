PORT=3001
JWT_SECRET=generate-a-random-secret-here

# OpenClaw Gateway
OPENCLAW_GATEWAY_URL=ws://127.0.0.1:18789
OPENCLAW_AUTH_TOKEN=your-openclaw-auth-token-here

# Google OAuth (optional — get from https://console.cloud.google.com/apis/credentials)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:3001/api/oauth/google/callback

# Microsoft OAuth (optional — get from https://portal.azure.com/#view/Microsoft_AAD_RegisteredApps)
MICROSOFT_CLIENT_ID=
MICROSOFT_CLIENT_SECRET=
MICROSOFT_REDIRECT_URI=http://localhost:3001/api/oauth/microsoft/callback
